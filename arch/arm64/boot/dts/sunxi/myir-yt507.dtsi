/{
	model = "sun50iw9 myir-yt507";
	compatible = "allwinner,t507", "arm,sun50iw9p1";

	reg_usb0_vbus: usb0-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb-vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		enable-active-high;
	};

	reg_usb1_vbus: usb1-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb-vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		enable-active-high;
	};

	reg_audio_vdd: audio_vdd {
                compatible = "regulator-fixed";
                regulator-name = "audio-vdd";
                regulator-min-microvolt = <3300000>;
                regulator-max-microvolt = <3300000>;
                enable-active-high;
        };

	 reg_audio_vdd1v8: audio_vdd1v8 {
                compatible = "regulator-fixed";
                regulator-name = "audio-vdd1v8";
                regulator-min-microvolt = <1800000>;
                regulator-max-microvolt = <1800000>;
                enable-active-high;
        };


	
       pw_en {
               compatible = "pw-en-gpio";
/*
               gpio0 = <&pio PE 11 1 0xffffffff 0xffffffff 1>;// 5v 3.3V
               gpio1 = <&pio PE 3 1 0xffffffff 0xffffffff 1>; //USB 5V
               gpio2 = <&pio PE 16 1 0xffffffff 0xffffffff 1>;// hub reset
               gpio3 = <&pio PE 10 1 0xffffffff 0xffffffff 1>;// fan
               gpio8 = <&pio PE 9 1 0xffffffff 0xffffffff 0>;// otg mode
               gpio9 = <&pio PE 2 1 0xffffffff 0xffffffff 0>;// otg pwr
	       pinctrl-0 = <&pwr_pins>;
*/
               status = "okay";
       };       
       
      rp_gpio{
              status = "okay";
              compatible = "rp_gpio";
	      /*GPIO LED*/
              //led {
              //    gpio_num = <&pio PE 19 1 0xffffffff 0xffffffff 1>;
              //    gpio_function = <3>;            //0:output 1:input 2:IRQ 3:flash
                  //gpio_event = <KEY_F14>;
                  //send_mode = <0>;  
              //};
	};
/*
	backlight {
        	status = "okay";
        	compatible = "pwm-backlight";
        	pwms = <&pwm0 0 50000 0>;
        	brightness-levels = <0 2 4 8 16 32 64 128 255>;
		defaults-brightness-level = <128>;
	};
*/
	leds {
		compatible = "gpio-leds";
    		pinctrl-names = "default";
    		status = "okay";
		
		led: led-blue {
			label = "blue";
			gpios = <&pio PE 19 1 1 1 0>;
			linux,defaults-trigger = "heartbeat";
			defaults-state = "on";
		};

	};
	
	i2c@0{
		compatible = "i2c-gpio";
		status = "okay";
		gpios = <&pio PD 26 1 1 1 0 /*SDA*/
			 &pio PD 25 1 1 1 0 /*SCL*/
			>;

		//i2c-gpio,sda-open-drain;
		//i2c-gpio,scl-open-drain;
		//i2c-gpio,delay-us = <2>;	/* ~100 kHz */
		#address-cells = <1>;
		#size-cells = <0>;

		edt-ft5x06@38 {
			compatible = "edt,edt-ft5306", "edt,edt-ft5x06";
			reg = <0x38>;
			interrupt-parent = <&pio>;
                        interrupts = <PD 24 IRQ_TYPE_LEVEL_LOW>;
                        gpio-controller;
			reset-gpios = <&pio PD 27 1 0xffffffff 0xffffffff 1>;
			status = "okay";
		};
	};	

	soc@03000000 {
		pio: pinctrl@0300b000 {

			/* takes the debounce time in usec as argument */
			input-debounce = <0 0 0 0 0 0 1>;
			};

		spi0: spi@05010000 {
			pinctrl-0 = <&spi0_pins_a &spi0_pins_b>;
			pinctrl-1 = <&spi0_pins_c>;
			spi_slave_mode = <0>;
			status = "disabled";
			spi_board0 {
				device_type = "spi_board0";
				compatible = "spidev";
				spi-max-frequency = <0x5f5e100>;
				reg = <0x0>;
			};
		};

		spi1: spi@05011000 {
			pinctrl-0 = <&spi1_pins_a &spi1_pins_b>;
			pinctrl-1 = <&spi1_pins_c>;
			spi_slave_mode = <0>;
			status = "disabled";
			spi_board1 {
				device_type = "spi_board1";
				compatible = "spidev";
				spi-max-frequency = <0x5f5e100>;
				reg = <0x0>;
				spi-rx-bus-width = <0x1>;
				spi-tx-bus-width = <0x1>;
			};
		};

		sdc2: sdmmc@04022000 {
		//	pinctrl-0 = <&sdc2_pins_a &sdc2_pins_c>;
			non-removable;
			bus-width = <8>;
			mmc-ddr-1_8v;
		//	mmc-hs200-1_8v;
			mmc-hs400-1_8v;
			no-sdio;
			no-sd;
			cap-mmc-highspeed;
			sunxi-power-save-mode;
			sunxi-dis-signal-vol-sw;
			max-frequency = <100000000>;
		//	ctl-spec-caps = <0x308>;
			vmmc-supply = <&reg_aldo1>;
			vqmmc-supply = <&reg_dcdc1>;
			status = "okay";
		};

		/*sdcard*/
		sdc0: sdmmc@04020000 {
			pinctrl-0 = <&sdc0_pins_a>;
			bus-width = <4>;
			cd-gpios = <&pio PF 6 6 1 3 0xffffffff>;
			cd-used-24M;
			/*non-removable;*/
			/*broken-cd;*/
			/*cd-inverted*/
			/*data3-detect;*/
			cap-sd-highspeed;
			sd-uhs-sdr50;
			sd-uhs-ddr50;
			sd-uhs-sdr104;
			no-sdio;
			no-mmc;
			sunxi-power-save-mode;
			sunxi-signal-vol-sw-without-pmu;
			/*sunxi-dis-signal-vol-sw;*/
			max-frequency = <150000000>;
			ctl-spec-caps = <0x8>;
			vmmc-supply = <&reg_dcdc1>;
			vqmmc33sw-supply = <&reg_dcdc1>;
			vdmmc33sw-supply = <&reg_dcdc1>;
			vqmmc18sw-supply = <&reg_aldo2>;
			vdmmc18sw-supply = <&reg_aldo2>;
			status = "okay";
		};

		/*wifi*/
		sdc1: sdmmc@04021000 {
			pinctrl-0 = <&sdc1_pins_a>;
			bus-width = <4>;
			no-mmc;
			no-sd;
			cap-sd-highspeed;
			/*sd-uhs-sdr12*/
			/*sd-uhs-sdr25*/
			/*sd-uhs-sdr50;*/
			/*sd-uhs-ddr50;*/
			/*sd-uhs-sdr104*/
			/*sunxi-power-save-mode;*/
			sunxi-dis-signal-vol-sw;
			cap-sdio-irq;
			keep-power-in-suspend;
			ignore-pm-notify;
			max-frequency = <50000000>;
			ctl-spec-caps = <0x8>;
			status = "okay";
		};
		
		/*Gethernet */
		gmac0: eth@05020000{
			phy-mode = "rgmii";
			phy-rst = <&pio PI 6 1 1 1 0>;
			use_ephy25m = <1>;
			tx-delay = <1>;
			rx-delay = <24>;
			//gmac-power0-supply = <&reg_cldo3>;
			//gmac-power1-supply;
			//gmac-power2-supply;
			//gmac-power0-vol = <3300000>;
			//gmac-power1-vol;
			//gmac-power2-vol;
			status = "okay";
		};

		/*megabit ethernet*/
		gmac1: eth@05030000{
			phy-mode = "rmii";
			phy-rst = <&pio PH 5 1 1 1 0>;
			use_ephy25m = <0>;
			tx-delay = <7>;
			rx-delay = <0>;
			gmac-power0-supply = <&reg_cldo3>;
			gmac-power1-supply;
			gmac-power2-supply;
			gmac-power0-vol = <3300000>;
			gmac-power1-vol;
			gmac-power2-vol;
			status = "okay";
		};
		
		/*twi0*/
		twi0: twi@0x05002000{
			clock-frequency = <400000>;
			pinctrl-0 = <&twi0_pins_a>;
			pinctrl-1 = <&twi0_pins_b>;
			twi-supply = <&reg_dcdc1>;
			status = "disabled";
		};
		
		/*twi1*/
		twi1: twi@0x05002400{
			clock-frequency = <200000>;
			pinctrl-0 = <&twi1_pins_a>;
			pinctrl-1 = <&twi1_pins_b>;
			twi-supply = <&reg_cldo3>;
			status = "disabled";

		};
		
		/*twi2*/
/*
		twi2: twi@0x05002800{
			clock-frequency = <200000>;
			pinctrl-0 = <&twi2_pins_a>;
			pinctrl-1 = <&twi2_pins_b>;
			twi-supply = <&reg_dcdc1>;
			status = "disabled";
			
		};
*/

		/*twi3*/
		twi3: twi@0x05002c00{
			clock-frequency = <200000>;
			pinctrl-0 = <&twi3_pins_a>;
			pinctrl-1 = <&twi3_pins_b>;
			twi-supply = <&reg_dcdc1>;
			status = "okay";
		};

		/*twi4*/
		twi4: twi@0x05003000{
			clock-frequency = <200000>;
			pinctrl-0 = <&twi4_pins_a>;
			pinctrl-1 = <&twi4_pins_b>;
			twi-supply = <&reg_cldo2>;
			status = "okay";
			
			sgtl5000@0a {
                                compatible = "allwinner,sgtl5000";
                                device_type = "sgtl5000";
                                reg = <0x0a>;
			
				VDDA-supply = <&reg_audio_vdd>;
				VDDIO-supply = <&reg_audio_vdd>;
				VDDD-supply = <&reg_audio_vdd1v8>;
                                micbias-resistor-k-ohms = <4>;
                                micbias-voltage-m-volts = <2250>;
				status = "okay";
                        };
		};

		/*twi5*/
		twi5: twi@0x07081400{
			clock-frequency = <200000>;
			pinctrl-0 = <&s_twi0_pins_a>;
			pinctrl-1 = <&s_twi0_pins_b>;
			status = "okay";

			
		};
		
		/*PWM*/
		pwm0: pwm0@0300a000 {
			pinctrl-names = "active", "sleep";
			pinctrl-0 = <&pwm0_pin_a>;
			pinctrl-1 = <&pwm0_pin_b>;
		};

		pwm5: pwm5@0300a000 {
               		pinctrl-names = "active", "sleep";
                        pinctrl-0 = <&pwm5_pin_a>;
                        pinctrl-1 = <&pwm5_pin_b>;

		 };

                
		/*GPADC*/
		/*
		gpadc:gpadc{
                        channel_num = <4>;
                        channel_select = <0x01>;
                        channel_data_select = <0>;
                        channel_compare_select = <0x01>;
                        channel_cld_select = <0x01>;
                        channel_chd_select = <0>;
                        channel0_compare_lowdata = <4700000>;
                        channel0_compare_higdata = <1000000>;
                        status = "okay";
                };
		*/

		/*BT*/
		uart1: uart@05000400 {
			status = "okay";
		};
		
		/*RS232*/
		uart2: uart@05000800 {
			uart-supply = <&reg_cldo3>;
			uart2_type = <2>;
			status = "okay";
		};

		/*NO USE*/
		uart3: uart@05000c00 {
			uart3_type = <2>;
			status = "disabled";
		};
		
		/*NO USE*/
		uart4: uart@05001000 {
			uart-supply = <&reg_cldo3>;
			uart4_type = <2>;
			status = "disabled";
		};

		/*RS485*/
		uart5: uart@05001400 {
			uart-supply = <&reg_cldo3>;
			uart5_type = <2>;
			status = "disabled";
		};

		wlan:wlan {
			compatible    = "allwinner,sunxi-wlan";
			clocks        = <&clk_losc_out>;
			pinctrl-0     = <&clk_losc_pins_a>;
			pinctrl-names = "default";
			wlan_busnum   = <0x1>;
			wlan_power    = "axp858-dcdc1";
			wlan_io_regulator = "axp858-cldo2";
			//wlan_regon    = <&pio PE 12 1 0xffffffff 0xffffffff 1>;
			//wlan_hostwake = <&pio PE 14 1 0xffffffff 0xffffffff 1>;
			chip_en;
			power_en;
			status        = "disabled";
		};

		bt:bt {
			compatible    = "allwinner,sunxi-bt";
			clocks        = <&clk_losc_out>;
			bt_power      = "axp858-dcdc1";
			bt_io_regulator = "axp858-cldo2";
			//bt_rst_n      = <&pio PE 13 1 0xffffffff 0xffffffff 0>;
			status        = "disabled";
		};

		btlpm:btlpm {
			compatible  = "allwinner,sunxi-btlpm";
			uart_index  = <0x1>;
		//	bt_wake     = <&pio PE 7 1 0xffffffff 0xffffffff 1>;
		//	bt_hostwake = <&pio PE 13 1 0xffffffff 0xffffffff 0>;
			status      = "disabled";
		};

		car_reverse:car_reverse {
			compatible  = "allwinner,sunxi-car-reverse";
			tvd_id  = <0x5>;
			screen_width = <1280>;
			screen_height = <800>;
			rotation = <0x0>;
			source = <0x1>;
			oview = <0x0>;
			reverse_pin = <&pio PI 15 6 0 1 0>;
			status      = "disabled";
		};

		addr_mgt:addr_mgt {
			compatible     = "allwinner,sunxi-addr_mgt";
			type_addr_wifi = <0x0>;
			type_addr_bt   = <0x0>;
			type_addr_eth  = <0x0>;
			status         = "okay";
		};

		usbc0:usbc0@0 {
			device_type		= "usbc0";
			usb_port_type		= <0x2>;
			usb_detect_type		= <0x1>;
			usb_detect_mode		= <0x0>;
			//usb_id_gpio		= <&pio PE 0 0 1 0 0>;
			//usb_det_vbus_gpio	= <&pio PE 1 0 1 0 0>;
			usb_regulator_io	= "nocare";
			usb_wakeup_suspend	= <0x0>;
			usb_luns		= <0x3>;
			usb_serial_unique	= <0x0>;
			usb_serial_number	= "20080411";
			status			= "okay";
		};

		ehci0:ehci0-controller@0x05101000 {
			drvvbus-supply = <&reg_usb0_vbus>;
		};

		ohci0:ohci0-controller@0x05101400 {
			drvvbus-supply = <&reg_usb0_vbus>;
		};

		usbc1:usbc1@0 {
			device_type		= "usbc1";
			usb_regulator_io	= "nocare";
			usb_wakeup_suspend	= <0x0>;
			status			= "okay";
		};

		ehci1:ehci1-controller@0x05200000 {
			drvvbus-supply = <&reg_usb1_vbus>;
		};

		ohci1:ohci1-controller@0x05200400 {
			drvvbus-supply = <&reg_usb1_vbus>;
		};

		usbc2:usbc2@0 {
			device_type		= "usbc2";
			usb_drv_vbus_gpio;
			usb_regulator_io	= "nocare";
			usb_wakeup_suspend	= <0x0>;
			status			= "okay";
		};

		usbc3:usbc3@0 {
			device_type		= "usbc3";
			usb_drv_vbus_gpio;
			usb_regulator_io	= "nocare";
			usb_wakeup_suspend	= <0x0>;
			status			= "okay";
		};

		/* audio dirver module */
		codec:codec@0x05096000 {
			adcdrc_cfg  = <0x00>;
			adchpf_cfg  = <0x00>;
			dacdrc_cfg  = <0x00>;
			dachpf_cfg  = <0x00>;
			fmin_gain   = <0x03>;
			linein_gain = <0x03>;
			digital_vol = <0x00>;
			lineout_vol = <0x1f>;//默认输出音量
			/* gpio-spk */
//			gpio-spk = <&pio PH 9 1 0xffffffff 0xffffffff 0>;
//			gpio-spk = <&pio PE 13 1 0xffffffff 0xffffffff 0>;
			pa_ctl_level   = <0x01>;
			pa_msleep_time = <0x78>;
			ramp_func_used = <0x01>;
			//avcc-supply = <&reg_aldo2>;
			status = "disabled";
		};

		spdif:spdif-controller@0x05093000{
			status = "disabled";
		};

		dmic:dmic-controller@0x05095000{
			status = "disabled";
		};

		ahub_codec:ahub_codec@0x05097000{
			status = "okay";
		};

		ahub_daudio0:ahub_daudio0@0x05097000{
			pinconfig 		= <0x01>;
			frametype 		= <0x00>;
			pcm_lrck_period		= <0x20>;
			slot_width_select	= <0x20>;
			daudio_master		= <0x04>;
			audio_format  		= <0x01>;
			signal_inversion 	= <0x01>;
			tdm_config		= <0x01>;
			mclk_div 		= <0x08>;
			status = "disabled";
		};

		ahub_daudio1:ahub_daudio1@0x05097000{
			pinconfig 		= <0x00>;
			frametype 		= <0x00>;
			pcm_lrck_period 	= <0x20>;
			slot_width_select 	= <0x20>;
			daudio_master 		= <0x04>;
			audio_format 		= <0x01>;
			signal_inversion 	= <0x01>;
			tdm_config 		= <0x01>;
			mclk_div 		= <0x01>;
			status = "disabled";
		};

		ahub_daudio2:ahub_daudio2@0x05097000{
			pinconfig 		= <0x01>;//0x01
			frametype 		= <0x00>;
			pcm_lrck_period 	= <0x10>;
			slot_width_select 	= <0x10>;
			daudio_master 		= <0x01>;
			audio_format 		= <0x01>;//01: standard i2s format
			signal_inversion 	= <0x01>;
			tdm_config 		= <0x01>;//00:pcm  01:i2s
			mclk_div 		= <0x04>;//0x04?分频为什么要这样分.默认24MHZ
			status = "okay";
		};

		ahub_daudio3:ahub_daudio3@0x05097000{
			pinconfig 		= <0x01>;
			frametype 		= <0x00>;
			pcm_lrck_period 	= <0x20>;
			slot_width_select 	= <0x20>;
			daudio_master 		= <0x04>;
			audio_format 		= <0x01>;
			signal_inversion 	= <0x01>;
			tdm_config 		= <0x01>;
			mclk_div 		= <0x08>;
			status = "disabled";
		};

		snddaudio3:sound@3{
			/* ac107-codec throught daudio0*/
			sunxi,snddaudio-codec = "ac107.3-0036";
			sunxi,snddaudio-codec-dai = "ac107-pcm0";
			status = "disabled";
		};

		/*
		sndhdmi:sound@1{
			status = "okay";
		};*/

		snddaudio2:sound@2{
			sunxi,snddaudio-codec = "sgtl5000.4-000a";
                        sunxi,snddaudio-codec-dai = "sgtl5000";
			status = "okay";
		};

		snddaudio0:sound@0{
			/* acx00-codec throught mfd_add_devices */
			/*
			sunxi,snddaudio-codec = "acx00-codec";
			sunxi,snddaudio-codec-dai = "acx00-dai";
			*/
			status = "disabled";
		};

		sndspdif:sound@4{
			status = "disabled";
		};

		snddmic:sound@5{
			status = "disabled";
		};

		sndcodec:sound@6{
			status = "okay";
		};

		sndahub:sound@7{
			status = "okay";
		};
/*--------------------------------------------------------------------------------
vin (video input modules) configuration
vind(x)_used: 0:disable 1:enable, main node for vin
csi(x)_used: 0:disable 1:enable
flash(x)_used: 0:disable 1:enable
flash(x)_type: 0:FLASH_RELATING, 1:FLASH_EN_INDEPEND, 2:FLASH_POWER
flash(x)_en: flash enable gpio, type = 0 of 1
flash(x)_mode: flash mode gpio, type = 0 of 1
flash(x)_flvdd: flash module io power handle string, pmu power supply, type = 2
flash(x)_flvdd_vol: flash module io power voltage, pmu power supply, type = 2
actuator(x)_used: 0:disable 1:enable
actuator(x)_name: vcm name
actuator(x)_slave: vcm iic slave address
actuator(x)_af_pwdn: vcm power down gpio
actuator(x)_afvdd: vcm power handle string, pmu power supply
actuator(x)_afvdd_vol: vcm power voltage, pmu power supply
sensor(x)_used: 0:disable 1:enable
sensor(x)_isp_used 0:not use isp 1:use isp
sensor(x)_fmt: 0:yuv 1:bayer raw rgb
sensor(x)_stby_mode: 0:not shut down power at standby 1:shut down power at standby
sensor(x)_vflip: flip in vertical direction 0:disable 1:enable
sensor(x)_hflip: flip in horizontal direction 0:disable 1:enable
sensor(x)_cameravdd-supply: camera module power handle string, pmu power supply
sensor(x)_cameravdd_vol: camera module power voltage, pmu power supply
sensor(x)_iovdd-supply: camera module io power handle string, pmu power supply
sensor(x)_iovdd_vol: camera module io power voltage, pmu power supply
sensor(x)_avdd-supply: camera module analog power handle string, pmu power supply
