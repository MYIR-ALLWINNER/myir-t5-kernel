/dts-v1/;
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include "sun50iw9p1-myir.dtsi"     
//#include "myir-lcd-1lvds-7-1024-600.dtsi"
//#include "myir-lcd-2lvds-7-1024-600-10-1280-800.dtsi"
//#include "myir-lcd-lvds-10.1-1280-800.dtsi" 

//#include "rp-lcd-lvds-10.1-1024-600.dtsi"
//#include "rp-lcd-rgb-7-1024-600.dtsi"
//#include "rp-lcd-lvds-21-1920-1080.dtsi"
//#include "rp-lcd-lvds-10.1-1024-600-hdmi.dtsi"
//#include "rp-lcd-lvds-10.1-1280-720.dtsi"
#include "rp-hdmi.dtsi"

//#include "myir-lvds-7-1024-600.dtsi"
//#include "myir-tv.dtsi"
