/*----------------------------------------------------------------------------------
;tv configuration
;dac_src            (dac no,support dac_src0~dac_src3,dac num max is 4)
;interface          (interface type,1<->cvbs,2<->YPBPR,4<->SVIDEO)
;dac_type           (0<->composite,1<->luma,2<->chroma,3<->reserved,4<->y/green,
;                    5<->u/pb/blue,6<->v/pr/red)
;NOTE:              tv0,tv1 can not use the same dac_src.
;---------------------------------------------------------------------------------*/
	tv0: tv0@01c94000 {
		status =  "okay";
		dac_src0        = <0>;
		dac_type0       = <0>;

		interface       = <1>;
		tv_power = "reg_cldo4";
		};
	};
	gpu: gpu@0x01800000 {
		gpu_idle = <0>;
		dvfs_status = <1>;
		independent_power = <1>;
		operating-points = <
                        /* KHz   uV */
			648000 960000
			600000 900000
			552000 880000
			504000 860000
			456000 840000
			420000 820000
			384000 820000
			360000 810000
			336000 810000
			306000 810000
		>;
		gpu-supply = <&reg_dcdc4>;
		};
	};

